{
    "name": "Anthropic Pharma",
    "fonts": {
        "heading": "Poppins",
        "body": "Lora"
    },
    "colors": {
        "primary": "#d97757",
        "secondary": "#6a9bcc",
        "tertiary": "#788c5d",
        "background": "#faf9f5",
        "text": "#141413",
        "header_text": "#ffffff",
        "borders": "#b0aea5"
    },
    "excel": {
        "show_gridlines": false,
        "header_borders": true,
        "alternating_rows": true
    },
    "analytics": {
        "rules": [
            {
                "column_pattern": "p-value|p_value|significance",
                "condition": "lessThan",
                "value": 0.05,
                "style": {
                    "bg_color": "c6efce",
                    "font_color": "006100"
                }
            },
            {
                "column_pattern": "yield|purity",
                "condition": "lessThan",
                "value": 90.0,
                "style": {
                    "bg_color": "ffc7ce",
                    "font_color": "9c0006"
                }
            }
        ]
    }
}